<% if (messages.success) { %>
 <div class="alert alert-success fade show">
     <button class="close" type="button" data-dismiss="alert">
        <i class="fas fa-times-circle"></i>
     </button>
     <% messages.success.forEach(success => { %>
      <div>
          <%= success.msg %>
      </div>
     <% }) %>
 </div>
<% } %>